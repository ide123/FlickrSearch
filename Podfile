# Uncomment the next line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
use_modular_headers!
use_frameworks!
target 'FlickrSearch' do
    pod 'RxSwift'
    pod 'RxCocoa', '~> 5'
    pod 'Alamofire', '~> 5.4'
    pod 'SwiftyJSON'
end
target 'FlickrSearchTests' do
  inherit! :search_paths
    pod 'RxBlocking', '~> 5'
    pod 'RxTest', '~> 5'
end
